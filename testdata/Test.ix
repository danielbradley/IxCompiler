public class extends ix.adt.Dictionary
{
    @type:    String&
    @values:  Info[]*
    %members: int
}

public new( type: String, size: int )
{
    @type   = type;
    @values = new Info[]
    %members++
}

public getValues(): Info[]&
{
    return @values;
}

public interest( value1: int, value2: int ): int
{
    return value1 * %members
}
